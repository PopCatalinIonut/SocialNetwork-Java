<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.socialnetwork.controller.FriendRequestsController">
    <children>
        <TableView fx:id="receivedTable" layoutY="113.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="280.0" prefWidth="300.0">
            <columns>
                <TableColumn fx:id="fromColumnReceivedTable" prefWidth="103.0" text="from" />
                <TableColumn fx:id="statusColumnReceivedTable" prefWidth="86.0" text="status" />
                <TableColumn fx:id="dateColumnReceivedTable" prefWidth="110.0" text="date" />
            </columns>
        </TableView>
      <Button layoutX="14.0" layoutY="23.0" mnemonicParsing="false" onAction="#handleAccept" prefHeight="31.0" prefWidth="93.0" text="Accept" />
      <Button layoutX="194.0" layoutY="23.0" mnemonicParsing="false" onAction="#handleReject" prefHeight="31.0" prefWidth="106.0" text="Reject" />
      <TableView fx:id="sentTable" layoutX="350.0" layoutY="113.0" prefHeight="280.0" prefWidth="250.0">
        <columns>
          <TableColumn fx:id="toColumnSentTable" prefWidth="123.0" text="to" />
          <TableColumn fx:id="dateColumnSentTable" prefWidth="126.0" text="date" />
        </columns>
      </TableView>
      <Button  layoutX="417.0" layoutY="23.0" onAction="#handleDeleteRequest" mnemonicParsing="false" text="Delete request" />
      <Label layoutX="115.0" layoutY="82.0" text="Received" />
      <Label layoutX="475.0" layoutY="82.0" text="Sent" />
    </children>
</AnchorPane>
